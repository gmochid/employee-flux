{
  "name": "todomvc-flux",
  "version": "0.0.3",
  "description": "Employee Application Flux",
  "repository": "https://github.com/gmochid/employee-flux",
  "main": "js/app.js",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.13.0",
    "newforms": "^0.12.1"
  },
  "devDependencies": {
    "browserify": "^9.0.7",
    "envify": "^3.0.0",
    "jest-cli": "^0.4.3",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^3.1.0",
    "gulp": "^3.9.0",
    "gulp-uglify": "^1.2.0",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.5",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-buffer": "^1.0.0",
    "lodash.assign": "^3.2.0"
  },
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
